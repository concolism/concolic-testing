ARTIFACT=unescape_string

TARGET:=$(ARTIFACT)

ifdef DEST_TOO_SMALL
BUGS+=-DBUG_DEST_TOO_SMALL
TARGET:=$(TARGET).DTS
endif

ifdef DEST_TOO_SMALL_BIS
BUGS+=-DBUG_DEST_TOO_SMALL_BIS
TARGET:=$(TARGET).DTS2
endif

ifdef ASCII_SOURCE
BUGS+=-DASCII_SOURCE # Not actually a bug...
TARGET:=$(TARGET).AS
endif

ifdef SM_COV_1
BUGS+=-DSM_COV_1
TARGET:=$(TARGET).S1
endif

ifdef SM_COV_2
BUGS+=-DSM_COV_2
TARGET:=$(TARGET).S2
endif

build:

include ../common.mk
