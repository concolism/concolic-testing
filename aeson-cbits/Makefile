ARTIFACT=unescape_string

TARGET:=$(ARTIFACT)

ifdef DEST_TOO_SMALL
BUGS+=-DBUG_DEST_TOO_SMALL
TARGET:=$(TARGET).DTS
endif

ifdef ASCII_SOURCE
BUGS+=-DASCII_SOURCE # Not actually a bug...
TARGET:=$(TARGET).AS
endif

ifdef UNICODE_SURROGATE
BUGS+=-DCOVER_UNICODE_SURROGATE
TARGET:=$(TARGET).US
endif

ifdef UNICODE_NO_SURROGATE
BUGS+=-DCOVER_UNICODE_NO_SURROGATE
TARGET:=$(TARGET).UNS
endif

ifdef COVER_BACKSLASH
BUGS+=-DCOVER_BACKSLASH
TARGET:=$(TARGET).CB
endif

ifdef COVER_EXIT
BUGS+=-DCOVER_EXIT
TARGET:=$(TARGET).CE
endif

ifdef LOW_CODEPOINT
BUGS+=-DCOVER_LOW_CODEPOINT
TARGET:=$(TARGET).LC
endif

build:

include ../common.mk
